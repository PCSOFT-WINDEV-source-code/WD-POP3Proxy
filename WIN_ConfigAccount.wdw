     name : ReadAccounts_OE
     internal_properties : CAAAAAgAAADs7lJkLTukaL6vWK5B8jMXHthJ1PYc+n88bDPoZ+YkzUmOYk6xw6QMD/cmygGk+DxaXxifkuiYxPcZVUMnAFjnmQPDslkoss8EAGbRQQnXiMZwpB5n2nYxz369Wo3nWk8I13pfAUXNSbf7GDAxD+ufGTw5gomIpojudyhhetghi9qC90C+4KHKxZe4S5uZnDqAcsz97cjzytL1xRCz2kN+xTqbA287iz5E7A9awg6KQ6vl7Gte36LoY/w9D3hEoJH2v9iS39kC6cdp2AXAugEFP4VCe78=
     procedure_id : 1760457526457008648
     type_code : 14
     code : |1+
      // Summary: Reads the available Outlook Express accounts
      // Syntax:
      //ReadAccounts_OE ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE ReadAccounts_OE()
      
      
      // Browses the registry
      sKey is string = RegistryFirstSubKey("HKEY_CURRENT_USER\Software\Microsoft\Internet Account Manager\Accounts")
      WHILE sKey <> ""
      	
      	// Reads the key
      	sPOP is string = RegistryQueryValue(sKey, "POP3 Server")
      	sUserPOP is string = RegistryQueryValue(sKey, "POP3 User Name")
      	sProp is string = RegistryQueryValue(sKey, "Account Name")
      	sEmail is string = RegistryQueryValue(sKey, "SMTP Email Address")
      	
      	// If it's a POP account
      	IF sPOP <> "" THEN
      		
      		// Is the account read already configured to use the proxy?
      		bUse is boolean = ((Position(sUserPOP, ":") > 0) OR (Position(sUserPOP, "@") > 0)) AND (sPOP ~= sGetListeningAddress())
      		TableAddLine(TABLE_OEAccount, sKey, bUse, bUse, sProp, sEmail, "OE")
      		
      	END
      	
      	// Next key
      	sKey = RegistryNextKey(sKey)
      	
      END
