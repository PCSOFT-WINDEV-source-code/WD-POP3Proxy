        internal_properties : CAAAAAgAAACOiAr0E0AIYJiozcudojkDJyWX8nOlLeWjWqNXeIs9+979brro20zqHFLop7wniv5XCXiOJKu9+2u3cLjlLXwk5fUGR/zs5suVPlgMnb3lx0+e6zbiKUdANlmorYr+mXaxKGV8C0hRM04t1G4GsvkAwyZOhAUh9j958scwzSGbQKJodWj2fBVfKix1bJfNGtRpMjjjjRz/R2ofCdJxVRSjtFi8rKTxtKuwNj6WsUWzw1YqQOkqdXJ75q3OWTMLT+sl0XBbXeVd77+Lgpvbx93EDmeBcg==
        code : |1+
         nAccount is int
         sPOP is string
         sUserPOP is string
         AccountInfo is a xmlDocument
         
         
         // Browses the table
         FOR nAccount = 1 _TO_ TableCount(TABLE_OEAccount)
         	
         	// If the account is configured to use the proxy
         	IF COL_bUseProxy[nAccount] THEN
         		
         		// If the account does not use the proxy yet
         		IF NOT COL_bUseCurrent[nAccount] THEN
         			
         			// Create the association
         			SWITCH COL_MessagingType
         				CASE "OE"
         					sPOP = RegistryQueryValue(COL_sAccountLoc[nAccount], <§$0001§>)
         					sUserPOP = RegistryQueryValue(COL_sAccountLoc[nAccount], <§$0002§>)
         					
         					// To do so, UserName becomes Server:UserName
         					RegistrySetValue(COL_sAccountLoc[nAccount], <§$0002§>, sPOP +":"+ sUserPOP)
         					RegistrySetValue(COL_sAccountLoc[nAccount], <§$0001§>, sGetListeningAddress())
         					
         				CASE "WLM"
         					AccountInfo = XMLOpen(COL_sAccountLoc[nAccount], fromFile)
         					IF ErrorOccurred = False THEN
         						// Modifies the information about the POP server and about the associated user
         						sPOP = AccountInfo.MessageAccount.POP3_Server
         						sUserPOP = AccountInfo.MessageAccount.POP3_User_Name
         						
         						AccountInfo.MessageAccount.POP3_User_Name = sUserPOP +"@"+ sPOP
         						AccountInfo.MessageAccount.POP3_Server = sGetListeningAddress()
         						XMLSave(AccountInfo)
         					END
         					
         			END
         			
         		END
         		
         	ELSE
         		
         		// If the account uses the proxy account
         		IF COL_bUseCurrent[nAccount] THEN
         			
         			SWITCH COL_MessagingType
         				CASE "OE"
         					// Remove the association
         					sPOP = RegistryQueryValue(COL_sAccountLoc[nAccount], <§$0001§>)
         					sUserPOP = RegistryQueryValue(COL_sAccountLoc[nAccount], <§$0002§>)
         					
         					// Restore the former parameters
         					// Split the identifier
         					nSeparPos is int = Position(sUserPOP, ":")
         					sServer, sUser are strings
         					IF nSeparPos>0 THEN
         						sServer = Left(sUserPOP, nSeparPos-1)
         						sUser = Middle(sUserPOP, nSeparPos+1)
         					ELSE
         						nSeparPos = Position(sUserPOP, "@")
         						sUser = Left(sUserPOP, nSeparPos-1)
         						sServer = Middle(sUserPOP, nSeparPos+1)
         					END
         					
         					// And replace the components in the registry
         					RegistrySetValue(TABLE_OEAccount.COL_sAccountLoc[nAccount], <§$0002§>, sUser)
         					RegistrySetValue(TABLE_OEAccount.COL_sAccountLoc[nAccount], <§$0001§>, sServer)
         					
         				CASE "WLM"
         					AccountInfo = XMLOpen(COL_sAccountLoc[nAccount], fromFile)
         					IF ErrorOccurred = False THEN
         						// Modifies the information about the POP server and about the associated user
         						sPOP = AccountInfo.MessageAccount.POP3_Server
         						sUserPOP = AccountInfo.MessageAccount.POP3_User_Name
         						
         						sPOP = ExtractString(sUserPOP, 2, "@")
         						sUserPOP = ExtractString(sUserPOP, 1, "@")
         						
         						AccountInfo.MessageAccount.POP3_User_Name = sUserPOP
         						AccountInfo.MessageAccount.POP3_Server = sPOP
         						XMLSave(AccountInfo)
         					END
         					
         			END
         			
         		END
         		
         	END
         	
         END
         
