       name : MenuPreviousWeek
       identifier : 0x286e66cf20ab10ae
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            dStartDate is date
            dEndDate is date
            sSuperControl is string
            
            // Retrieve the name of the super control
            sSuperControl = ExtractString(ControlPopupOwner(), 1, ".")
            
            // Position the day on the previous week
            dStartDate..Day -= 6
            
            {sSuperControl+<§$0008§>,indControl} = dStartDate
            {sSuperControl+<§$0009§>,indControl} = IntegerToDayInAlpha(dStartDate)
            
            {sSuperControl+<§$000a§>,indControl} = dEndDate
