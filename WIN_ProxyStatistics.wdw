           type : 32
     -
       name : Previous_week
       identifier : 0x286e66cf20ac10ae
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            dStartDate is date
            dEndDate is date
            nDayNum is int
            sSuperControl is string
            
            // Retrieve the name of the super control
            sSuperControl = ExtractString(ControlPopupOwner(), 1, ".")
            
            // Previous week
            dStartDate..Day -= 7
            nDayNum = IntegerToDay(dStartDate)
            dStartDate..Day -= (nDayNum-1)
            dEndDate = dStartDate
            dEndDate += 6
            
            {sSuperControl+<§$0008§>,indControl} = dStartDate
            {sSuperControl+<§$0009§>,indControl} = IntegerToDayInAlpha(dStartDate)
            
            {sSuperControl+<§$000a§>,indControl} = dEndDate
