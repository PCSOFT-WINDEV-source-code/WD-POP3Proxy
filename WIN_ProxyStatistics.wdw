     -
       name : MenuCurrentWeek
       identifier : 0x286e66cf20aa10ae
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            dStartDate is date = DateSys()
            dEndDate is date = DateSys()
            nDayNum is int
            sSuperControl is string
            
            // Retrieve the name of the super control
            sSuperControl = ExtractString(ControlPopupOwner(), 1, ".")
            
            // Retrieve the number of the day of the week
            nDayNum = IntegerToDay(dStartDate)
            
            // Current week
            dStartDate..Day -= (nDayNum-1)
            
            {sSuperControl+<§$0008§>,indControl} = dStartDate
            {sSuperControl+<§$0009§>,indControl} = IntegerToDayInAlpha(dStartDate)
            
            {sSuperControl+<§$000a§>,indControl} = dEndDate
            {sSuperControl+<§$000b§>,indControl} = IntegerToDayInAlpha(dEndDate)
            
            {sSuperControl,indControl}..Value = dStartDate+TAB+dEndDate
