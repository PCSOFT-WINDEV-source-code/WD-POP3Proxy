   -
     name : ReadAccounts_WLM
     internal_properties : CAAAAAgAAABjbEB3H7u662cNgUwjjVXYUaoTi3iV1tsvzHya32Bjo+laBTLEKoR79UGoc35Tq0B6W4WlWSwMmrJO+p+YlwijDQVynXnKxqQdVG3dtiGNp923MPZWCG2uU72omPnpfkjhIQpmtp6n5nTqJP0OZXSNIbKkIcgAPiOCoiRUjQZSq2bSZMlLJrvlcRoENmgPDwNDbcRxTShxzZRPqYJnc9qG12ZkzqhFlHnvQLaelH1DUaA+Mn/BDZNsbwLW1BVq2n9w8A==
     procedure_id : 1760457526457074184
     type_code : 14
     code : |1+
      // Summary: Reads the available Windows Live Mail accounts
      // Syntax:
      //ReadAccounts_WLM ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE ReadAccounts_WLM()
      
      sDirectory is string
      sListFile is string
      AccountInfo is a xmlDocument
      sUserPOP is string
      sPop is string
      sProp is string 
      sEmail is string
      
      
      // Defines the default directory for the files of Windows Live Mail
      sDirectory = SysDir(srLocalAppData) +[fSep()]+ "Microsoft\Windows Live Mail\"
      
      // Finds the files whose extension is .oeaccount
      sListFile = fListFile(sDirectory +[fSep()]+ "*.oeaccount", frRecursive)
      FOR EACH string sAFile of sListFile SEPARATED BY CR
      	
      	// Valid path only
      	IF sAFile IN ("", EOT) THEN CONTINUE
      	
      	// An oeaccount file is an XML file and therefore it can be handled like an XML file
      	AccountInfo = XMLOpen(sAFile, fromFile)
      	IF ErrorOccurred = False THEN
      		
      		// Retrieves the information about the POP account
      		sPop = AccountInfo.MessageAccount.POP3_Server
      		sUserPOP = AccountInfo.MessageAccount.POP3_User_Name
      		sProp = AccountInfo.MessageAccount.Account_Name
      		sEmail = AccountInfo.MessageAccount.SMTP_Email_Address
      		
      		// If the POP server is not specified (case of a .oeaccount file for reading news for example...)
      		IF sPop <> "" THEN
      			
      			bUse is boolean = ((Position(sUserPOP, ":") > 0) OR (Position(sUserPOP, "@") > 0)) AND (sPop ~= sGetListeningAddress())
      			TableAddLine(TABLE_OEAccount, sAFile, bUse, bUse, sProp, sEmail, "WLM")
      			
      		END
      		
      	END
      	
      END
